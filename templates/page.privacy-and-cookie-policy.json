      "blocks": {
        "content_block_AJTeCp": {
          "type": "content_block",
          "settings": {
            "heading": "Security",
            "content": "<p>We are committed to ensuring that your information is secure. In order to prevent unauthorised access or disclosure, we have put in place suitable physical, electronic and managerial procedures to safeguard and secure the information we collect online.</p>",
            "show_list": false,
            "list_item_1": "",
